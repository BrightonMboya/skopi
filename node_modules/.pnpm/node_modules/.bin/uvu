#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/tony/web-apps/skope/node_modules/.pnpm/uvu@0.5.6/node_modules/uvu/node_modules:/home/tony/web-apps/skope/node_modules/.pnpm/uvu@0.5.6/node_modules:/home/tony/web-apps/skope/node_modules/.pnpm/node_modules:/home/tony/web-apps/skope/node_modules:/home/tony/web-apps/node_modules:/home/tony/node_modules:/home/node_modules:/node_modules:/home/tony/web-apps/skope/node_modules/.pnpm/node_modules/uvu/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/tony/web-apps/skope/node_modules/.pnpm/uvu@0.5.6/node_modules/uvu/node_modules:/home/tony/web-apps/skope/node_modules/.pnpm/uvu@0.5.6/node_modules:/home/tony/web-apps/skope/node_modules/.pnpm/node_modules:/home/tony/web-apps/skope/node_modules:/home/tony/web-apps/node_modules:/home/tony/node_modules:/home/node_modules:/node_modules:/home/tony/web-apps/skope/node_modules/.pnpm/node_modules/uvu/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../uvu/bin.js" "$@"
else
  exec node  "$basedir/../uvu/bin.js" "$@"
fi
