#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/tony/web-apps/skope/node_modules/.pnpm/vscode-languageserver@8.0.2/node_modules/vscode-languageserver/bin/node_modules:/home/tony/web-apps/skope/node_modules/.pnpm/vscode-languageserver@8.0.2/node_modules/vscode-languageserver/node_modules:/home/tony/web-apps/skope/node_modules/.pnpm/vscode-languageserver@8.0.2/node_modules:/home/tony/web-apps/skope/node_modules/.pnpm/node_modules:/home/tony/web-apps/skope/node_modules:/home/tony/web-apps/node_modules:/home/tony/node_modules:/home/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/tony/web-apps/skope/node_modules/.pnpm/vscode-languageserver@8.0.2/node_modules/vscode-languageserver/bin/node_modules:/home/tony/web-apps/skope/node_modules/.pnpm/vscode-languageserver@8.0.2/node_modules/vscode-languageserver/node_modules:/home/tony/web-apps/skope/node_modules/.pnpm/vscode-languageserver@8.0.2/node_modules:/home/tony/web-apps/skope/node_modules/.pnpm/node_modules:/home/tony/web-apps/skope/node_modules:/home/tony/web-apps/node_modules:/home/tony/node_modules:/home/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/installServerIntoExtension" "$@"
else
  exec node  "$basedir/../../bin/installServerIntoExtension" "$@"
fi
