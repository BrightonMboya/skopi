#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/tony/web-apps/skope/node_modules/.pnpm/@babel+parser@7.20.7/node_modules/@babel/parser/bin/node_modules:/home/tony/web-apps/skope/node_modules/.pnpm/@babel+parser@7.20.7/node_modules/@babel/parser/node_modules:/home/tony/web-apps/skope/node_modules/.pnpm/@babel+parser@7.20.7/node_modules/@babel/node_modules:/home/tony/web-apps/skope/node_modules/.pnpm/@babel+parser@7.20.7/node_modules:/home/tony/web-apps/skope/node_modules/.pnpm/node_modules:/home/tony/web-apps/skope/node_modules:/home/tony/web-apps/node_modules:/home/tony/node_modules:/home/node_modules:/node_modules:/home/tony/web-apps/skope/node_modules/.pnpm/@babel+core@7.20.12/node_modules/@babel/core/node_modules:/home/tony/web-apps/skope/node_modules/.pnpm/@babel+core@7.20.12/node_modules/@babel/node_modules:/home/tony/web-apps/skope/node_modules/.pnpm/@babel+core@7.20.12/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/tony/web-apps/skope/node_modules/.pnpm/@babel+parser@7.20.7/node_modules/@babel/parser/bin/node_modules:/home/tony/web-apps/skope/node_modules/.pnpm/@babel+parser@7.20.7/node_modules/@babel/parser/node_modules:/home/tony/web-apps/skope/node_modules/.pnpm/@babel+parser@7.20.7/node_modules/@babel/node_modules:/home/tony/web-apps/skope/node_modules/.pnpm/@babel+parser@7.20.7/node_modules:/home/tony/web-apps/skope/node_modules/.pnpm/node_modules:/home/tony/web-apps/skope/node_modules:/home/tony/web-apps/node_modules:/home/tony/node_modules:/home/node_modules:/node_modules:/home/tony/web-apps/skope/node_modules/.pnpm/@babel+core@7.20.12/node_modules/@babel/core/node_modules:/home/tony/web-apps/skope/node_modules/.pnpm/@babel+core@7.20.12/node_modules/@babel/node_modules:/home/tony/web-apps/skope/node_modules/.pnpm/@babel+core@7.20.12/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../@babel+parser@7.20.7/node_modules/@babel/parser/bin/babel-parser.js" "$@"
else
  exec node  "$basedir/../../../../../../@babel+parser@7.20.7/node_modules/@babel/parser/bin/babel-parser.js" "$@"
fi
