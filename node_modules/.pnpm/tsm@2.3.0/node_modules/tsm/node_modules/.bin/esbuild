#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/tony/web-apps/skope/node_modules/.pnpm/esbuild@0.15.18/node_modules/esbuild/bin/node_modules:/home/tony/web-apps/skope/node_modules/.pnpm/esbuild@0.15.18/node_modules/esbuild/node_modules:/home/tony/web-apps/skope/node_modules/.pnpm/esbuild@0.15.18/node_modules:/home/tony/web-apps/skope/node_modules/.pnpm/node_modules:/home/tony/web-apps/skope/node_modules:/home/tony/web-apps/node_modules:/home/tony/node_modules:/home/node_modules:/node_modules:/home/tony/web-apps/skope/node_modules/.pnpm/tsm@2.3.0/node_modules/tsm/node_modules:/home/tony/web-apps/skope/node_modules/.pnpm/tsm@2.3.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/tony/web-apps/skope/node_modules/.pnpm/esbuild@0.15.18/node_modules/esbuild/bin/node_modules:/home/tony/web-apps/skope/node_modules/.pnpm/esbuild@0.15.18/node_modules/esbuild/node_modules:/home/tony/web-apps/skope/node_modules/.pnpm/esbuild@0.15.18/node_modules:/home/tony/web-apps/skope/node_modules/.pnpm/node_modules:/home/tony/web-apps/skope/node_modules:/home/tony/web-apps/node_modules:/home/tony/node_modules:/home/node_modules:/node_modules:/home/tony/web-apps/skope/node_modules/.pnpm/tsm@2.3.0/node_modules/tsm/node_modules:/home/tony/web-apps/skope/node_modules/.pnpm/tsm@2.3.0/node_modules"
fi
"$basedir/../../../../../esbuild@0.15.18/node_modules/esbuild/bin/esbuild"   "$@"
exit $?
